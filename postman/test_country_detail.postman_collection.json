	"item": [
		{
			"name": "Test get country details",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('General status code is 200', function () {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test('Response status code is 200', function () {\r",
							"  pm.expect(pm.response.json().statusCode).to.eql(200);\r",
							"});\r",
							"pm.test('Response object has data key and not error key', function () {\r",
							"  pm.expect(pm.response.json()).to.have.all.keys('statusCode','transactionTimestamp','data');\r",
							"  pm.expect(pm.response.json()).to.not.have.keys('error');\r",
							"});\r",
							"pm.test('Response object data has minimun keys', function () {\r",
							"  pm.expect(pm.response.json().data.name).not.null;\r",
							"  pm.expect(pm.response.json().data.isoCode).not.null;\r",
							"});\r",
							""
